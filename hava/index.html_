	<!-- Script från JQuery, till smooth scroll till menyn-->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script>
    $(document).ready(function(){
    // Lägg till mjuk scroll till alla länkar "a" när man klickar på e
    $("a").on('click', function(event) {

        // Make sure this.hash has a value before overriding default behavior
        if (this.hash !== "") {
        // Prevent default anchor click behavior
        event.preventDefault();

        // Store hash
        var hash = this.hash;

        // jQuerys animation för mjuk scroll
        // Nummret är hur många millisekunder det tar till den valda sektionen
        $('html, body').animate({
            scrollTop: $(hash).offset().top
        }, 1200, function(){
    
        });
        } 
    });
    });
	</script>
	
